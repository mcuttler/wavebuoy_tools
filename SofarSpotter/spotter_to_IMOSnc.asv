function [] = spotter_to_IMOSnc(outpathNC, filenameNC)  


%global attributes
fileattrib(filenameNC,'+w');
ncwriteatt('myfile.nc','/','creation_date',datestr(now));


%variable attributes
fileattrib(filenameNC,'+w');

ncwriteatt('myfile.nc','peaks','description','Output of PEAKS');

